{
  "build": {
    "content": [
      {
        "files": [
          "**/*.yml"
        ],
        "exclude": [
          "**/toc.yml"
        ],
        "src": "docs-ref-autogen",
        "dest": "."
      },
      {
        "files": [
          "toc.yml"
        ],
        "src": "docs-ref-autogen",
        "dest": "."
      },
      {
        "files": [
          "index.md"
        ]
      },
      {
        "files": [
          "toc.yml"
        ],
        "src": "breadcrumb",
        "dest": "breadcrumb"
      },
      {
        "files": [
          "**/*.md"
        ],
        "src": "docs-ref-conceptual",
        "dest": "docs-ref-conceptual"
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg"
        ],
        "exclude": [
          "**/obj/**",
          "site/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": "docs-ref-overwrite/*.md",
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "~/breadcrumb/toc.yml"
    },
    "fileMetadata": {
    },
    "template": [
      "default"
    ],
    "dest": "site"
  }
}